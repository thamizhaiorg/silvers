{
  "expo": {
    "name": "Silver",
    "slug": "silverapp",
    "version": "1.0.0",
    "scheme": "silver-pos",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/silvers.png",
    "splash": {
      "image": "./assets/silvers.png",
      "resizeMode": "contain",
      "backgroundColor": "#378388"
    },
    "platforms": [
      "android"
    ],
    "android": {
      "package": "com.silver.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/silvers.png",
        "backgroundColor": "#378388"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": false,
          "root": "./src/app"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your products.",
          "cameraPermission": "The app accesses your camera to let you take photos for your products."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false,
        "root": "./src/app"
      },
      "eas": {
        "projectId": "cc294013-1344-4934-8554-0d3cce89d739"
      }
    },
    "owner": "thamizhaiorg"
  }
}
